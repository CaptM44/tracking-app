{
	"manifest_version": 2,
	"name": "Tracker App",
	"version": "1.0.0",
	"page_action": {
		"default_icon": "img.png",
		"default_popup": "popup.html",
		"default_title": "My custom google page!"
	},
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"permissions": ["activeTab", "declarativeContent", "storage"],
	"options_page": "options.html"
}